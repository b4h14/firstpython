#dicionários - feito pra armazenar dados em pares e pode ser alterado
  novoDicionario = {Dado1:Resposta1, Dados1:Respostas1, Chave1:Valor1 ...}
  
    #query - Fazer uma consulta do valor com base no índice ou na chave
      dict[indiceValor]
    #Incluir algum valor no dicionario
      dict["Chave"] = Valor
    #del - Deletar um valor no dicionário
      del dict["Chave"]
  
#listas - ordenadas, alteráveis, indexadas
  #o mais usado em objetos simples
    novaLista = [Data1,Data2,Data3...]
  #é possível criar uma sublista dentro da lista
    novaLista = [Data1,Data2,[SubData1,SubData2]]

#tuplas - ordenadas, permitem registros duplicados e não pode ser alterado
  novaTupla = (Data1,Data1,Data2,Data3...)
    
    #zip - separa uma coleção e faz uma iteração criando tuplas
      Lista1 = [1,2,3,4]
      Lista2 = [a,b,c,d]
      Lista3 = zip(Lista1,Lista2)
        #output= (1,a) (2,b) (3,c) (4,d)

#set - não é indexável, nem alterável (somente para incluir e excluir) e não é ordenado
  novoSet = {Data1, Data2, Data3...)
    
#é possível consultar indices de listas, tuplas ou dicionarios usando
  lista[0] || tupla[-1] || lista[0:-2]
    #O indice sempre começa com 0
    #Para incluir um intervalo de indices, use o primeiro intervalo fechado e o segundo aberto
  
